<Application x:Class="WPF_Rich_Text_Editor_Homework.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:winforms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             xmlns:local="clr-namespace:WPF_Rich_Text_Editor_Homework"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <BitmapImage x:Key="Color Bucket" UriSource="assets\icons8-fill-color-19.png"/>
            <BitmapImage x:Key="Text Color" UriSource="assets\icons8-text-color-26.png"/>
            <BitmapImage x:Key="Drop Down" UriSource="assets\icons8-down-96.png"/>
            <BitmapImage x:Key="Save" UriSource="assets\icons8-save-64.png"/>
            <BitmapImage x:Key="Cut" UriSource="assets\icons8-cut-96.png"/>
            <BitmapImage x:Key="Copy" UriSource="assets\icons8-copy-50.png"/>
            <BitmapImage x:Key="Paste" UriSource="assets\icons8-paste-22.png"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
